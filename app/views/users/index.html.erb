
<table>
  <thead>
    <tr>
      
      <th>Username</th>
      <th>Email</th>
      <th>DOB</th>
      <th>Address</th>
      <th>Phone</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.dob %></td>
        <td><%= user.address %></td>
        <td><%= user.phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p> <%= link_to 'Back', root_url %></p>
